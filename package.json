{
  "engines": {
    "node": "19.8.1"
  },
  "name": "bloglist-app",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node index.js",
    "build": "cd client && npm run build",
    "dev": "concurrently --kill-others-on-fail \"NODE_ENV=development nodemon index.js\" \"cd client && npm start\"",
    "test": "NODE_ENV=test jest --verbose --runInBand --detectOpenHandles && cd client && CI=true npm test",
    "lint": "eslint './**/*.{js,jsx}'",
    "start:test": "concurrently --kill-others-on-fail \"NODE_ENV=test node index.js\" \"cd client && npm start\"",
    "test:e2e": "cd client && npm run cypress:run"
  },
  "author": "Ahmed Kasu",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^8.22.0",
    "eslint-plugin-cypress": "^2.13.3",
    "eslint-plugin-jest": "^27.2.2",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^28.1.3",
    "nodemon": "^2.0.19",
    "supertest": "^6.2.4"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^8.2.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.5.2"
  },
  "jest": {
    "testEnvironment": "node",
    "modulePathIgnorePatterns": [
      "client"
    ]
  }
}
